{
  "name": "n8n-nodes-aave",
  "version": "1.0.0",
  "description": "n8n community node for Aave Protocol - DeFi lending and borrowing operations across multiple chains",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "aave",
    "defi",
    "lending",
    "borrowing",
    "flash-loans",
    "ethereum",
    "polygon",
    "arbitrum",
    "optimism",
    "blockchain",
    "web3",
    "cryptocurrency",
    "smart-contracts",
    "gho",
    "staking"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://velobpa.com",
  "author": {
    "name": "Velocity BPA, LLC",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aave.git"
  },
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aave/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AaveNetwork.credentials.js",
      "dist/credentials/AaveApi.credentials.js",
      "dist/credentials/PriceOracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Aave/Aave.node.js",
      "dist/nodes/Aave/AaveTrigger.node.js"
    ]
  },
  "dependencies": {
    "@aave/contract-helpers": "^1.25.0",
    "@aave/math-utils": "^1.25.0",
    "axios": "^1.6.2",
    "ethers": "^5.7.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
